#include <stdio.h>
#include <string.h>

int main (void) {

  char Answer[15];

  char *Pickup[] = {
        "Did you fart?",
        "Are you http?",
        "Are you my phone screen?"
  };
  char *Line[] = {
        "Cause you blew me away!",
        "Because without you I'm just ://",
        "Cause I wanna touch you every second."
  };

  printf("%s", Pickup[1]);
  printf("\n");
  
  scanf("%s", Answer);
  printf("\n");
  
  printf("%s", Line[1]);

  return 0;
  
}